<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions name="SI_KCRM_PROCESS_OUTBOUND"
                  targetNamespace="http://bu.edu/sap/kcrm" xmlns:rfc="urn:sap-com:document:sap:rfc:functions"
                  xmlns:p1="http://bu.edu/sap/kcrm" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
                  xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
    <wsdl:documentation/>
    <wsp:UsingPolicy wsdl:required="true"/>
    <wsp:Policy wsu:Id="OP_SI_KCRM_PROCESS_OUTBOUND"/>
    <wsdl:types>
        <xsd:schema targetNamespace="urn:sap-com:document:sap:rfc:functions"
                    xmlns="urn:sap-com:document:sap:rfc:functions" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element name="Z_GM_KCRM_INTERFACE">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="BILLING_PLAN">
                            <xsd:annotation>
                                <xsd:documentation>Billing Plan Structure for interface
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZGMBILLINGPLANSTRUCTURE"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="GRANT" type="ZGMGRANTSTRUCTURE">
                            <xsd:annotation>
                                <xsd:documentation>Grants Master data Structure
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SPONSOR">
                            <xsd:annotation>
                                <xsd:documentation>Sponsor structure for KCRM Interface
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZGMSPONSORSTRUCTURE"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SPONSORED_PROGRAMS">
                            <xsd:annotation>
                                <xsd:documentation>Sponsored Program table type for interface
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZGMSPPROGRAMSTRUCTURE"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SPONSORED_PROGRAMS_GRP">
                            <xsd:annotation>
                                <xsd:documentation>Sponsored Program Group table type for
                                    Interface
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZGMSPPROGRAMGRPSTRUCTURE"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="RETURN" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Return Parameter</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="BAPIRET2" minOccurs="0"
                                                 maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Z_GM_KCRM_INTERFACE.Response">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="GRANT_MESSAGES" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Return Messages Table Type for KCRM Interface
                                    - Grant
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZGRANTMESSAGES"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SPONSORED_PROGRAMS_MESSAGES"
                                     minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Return Messages Table Type for KCRM Interface
                                    - SP Program
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZSPPROGRAMMESSAGES"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SPONSOR_MESSAGES" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Return Messages Table Type for KCRM Interface
                                    - Sponsor
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZSPONSORMESSAGES"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SP_XWALK_T" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Fund-Unit-Dept-Source_Prefix-Source-Suffix
                                    table type
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="ZFI_KCRM_SP_XWALK"
                                                 minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="RETURN" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Return Parameter</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="item" type="BAPIRET2" minOccurs="0"
                                                 maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:simpleType name="date">
                <xsd:union memberTypes="xsd:date genericDate" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                           xmlns="urn:sap-com:document:sap:rfc:functions"/>
            </xsd:simpleType>
            <xsd:simpleType name="genericDate">
                <xsd:restriction base="xsd:string">
                    <xsd:pattern value="....-..-.."/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="ZGRANTMESSAGES">
                <xsd:annotation>
                    <xsd:documentation>Return Messages for KCRM Interface - Grant
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="GRANT_NBR" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Grant</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="TYPE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message type: S Success, E Error, W Warning, I
                                Info, A Abort
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Text</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="220"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZBAPI0035FUNDST">
                <xsd:annotation>
                    <xsd:documentation>table of funds for grant master
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="item" type="ZBAPI0035FUNDS"
                                 minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZBAPI0035HEADERADD">
                <xsd:annotation>
                    <xsd:documentation>Additional data for grant master creation
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="BILLING_RULE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Rule Used as a Basis for Billing
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SALESORG" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Sales Organization</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="DISTR_CHAN" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Distribution Channel</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="2"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="DIVISION" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Division</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="2"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="WBS_ELEMENT" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Work Breakdown Structure Element (WBS Element)
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="24"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="ORDERID" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Order Number</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="12"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="INSTALLMENT_AMT" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Currency amount in BAPI interfaces
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="23"/>
                                <xsd:fractionDigits value="4"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MILESTONE_TOTAL" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Currency amount in BAPI interfaces
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="23"/>
                                <xsd:fractionDigits value="4"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="AUTH_GROUP" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Grants Management: Authorization Groups
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="10"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="AWARD_TYPE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Award Type</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="3"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="GRANT_TOTAL" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Currency amount in BAPI interfaces
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="23"/>
                                <xsd:fractionDigits value="4"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="CFDA_NBR" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Number in Catalog of Federal Domestic
                                Assistance
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="10"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="EXCHANGE_RATE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Grant exchange rate</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="9"/>
                                <xsd:fractionDigits value="5"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="EXCHANGE_DATE" type="date"
                                 minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Exchange Rate Date</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="FROM_FACTOR" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Ratio for the "From" Currency Units
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="9"/>
                                <xsd:fractionDigits value="0"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="TO_FACTOR" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Ratio for the "To" Currency Units
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="9"/>
                                <xsd:fractionDigits value="0"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="FY_VARIANT" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Fiscal year variant of grant
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="2"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="IDC_RULE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Rule for Determining Indirect Costs
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SPLITTING_RULE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Rule Used to Split Time Intervals Between
                                Fiscal Years
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="CS_RULE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Rule for Determining Cost Sharing Requirements
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="FUNDING_ORIGIN" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Origin of Grant Funding</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="30"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="BLOCKING_IND" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Blocking Indicator</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="PRE_AWARD_COSTS" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Pre-award cost control</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="LETTER_OF_CREDIT" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Letter of Credit</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="PAYMENT_RULE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Payment Rule</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="4"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="CS_RATE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Overall cost sharing rate for grant
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:totalDigits value="7"/>
                                <xsd:fractionDigits value="3"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ZBAPI0035SPONSOREDOBJECTS">
                <xsd:annotation>
                    <xsd:documentation>Grant master sponsored objects
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="FUND" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Fund</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="10"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SPONSORED_PROG" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Sponsored Program</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="SPONSORED_CLASS" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Sponsored Class</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="PLAN_ALLOWED" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Planning allowed indicator</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="BUDGET_ALLOWED" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Budgeting Allowed Indicator
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="ACTUALS_ALLOWED" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Actual Postings Allowed Indicator
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="BAPIRET2">
                <xsd:annotation>
                    <xsd:documentation>Return Parameter</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="TYPE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message type: S Success, E Error, W Warning, I
                                Info, A Abort
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="1"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="ID" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Class</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="NUMBER" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Number</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="3"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Text</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="220"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="LOG_NO" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Application log: log number
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="20"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="LOG_MSG_NO" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Application log: Internal message serial
                                number
                            </xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="6"/>
                                <xsd:pattern value="\d+"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE_V1" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Variable</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="50"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE_V2" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Variable</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="50"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE_V3" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Variable</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="50"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="MESSAGE_V4" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Message Variable</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
